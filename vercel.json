{
    "version": 2,
    "builds": [
      { "src": "*.html", "use": "@vercel/static" },
      { "src": "js/*.js", "use": "@vercel/static" },
      { "src": "*.css", "use": "@vercel/static" },
      { "src": "api/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/socket", "dest": "/api/socket.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "functions": {
        "api/socket.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "analytics": true
}
